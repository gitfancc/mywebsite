package com.appscomm.sport.vo;

import java.io.Serializable;

public class PersonalSetting implements Serializable{
	private static final long serialVersionUID = -6668477788300564840L;
	private Long id = 0L;
	private long personId;
	private String property;
	private String value;
	public long getPersonId() {
		return personId;
	}
	public void setPersonId(long personId) {
		this.personId = personId;
	}
	public String getProperty() {
		return property;
	}
	public void setProperty(String property) {
		this.property = property;
	}
	public String getValue() {
		return value;
	}
	public void setValue(String value) {
		this.value = value;
	}
	
	public Long getId() {
		return id;
	}
	public void setId(Long id) {
		this.id = id;
	}
	
	@Override
	public String toString() {
		return "PersonalSetting [id=" + id + ", personId=" + personId + ", property=" + property + ", value=" + value + "]";
	}
}
