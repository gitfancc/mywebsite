package com.appscomm.sport.model;

import java.io.Serializable;
import java.sql.Timestamp;

public class PersonWatchVO implements Serializable{

	private static final long serialVersionUID = -2646284078460045968L;
	
	public static final int STATUS_REALES=0;
	public static final int STATUS_BIND=1;
	
	public static final int IS_DEFAULT=1;
	public static final int IS_NOT_DEFAULT=0;
	
	private Integer id;
	private Long userId;
	private String watchId;
	private String watchSim;
	private Timestamp bindTime;
	private String watchType;
	private Integer active;
	private Integer isDefault;
	
	private String userAccount;
	
	public Long getUserId() {
		return userId;
	}
	public void setUserId(Long userId) {
		this.userId = userId;
	}
	public String getWatchId() {
		return watchId;
	}
	public void setWatchId(String watchId) {
		this.watchId = watchId;
	}
	public String getWatchSim() {
		return watchSim;
	}
	public void setWatchSim(String watchSim) {
		this.watchSim = watchSim;
	}
	public Timestamp getBindTime() {
		return bindTime;
	}
	public void setBindTime(Timestamp bindTime) {
		this.bindTime = bindTime;
	}
	public String getWatchType() {
		return watchType;
	}
	public void setWatchType(String watchType) {
		this.watchType = watchType;
	}
	public Integer getActive() {
		return active;
	}
	public void setActive(Integer active) {
		this.active = active;
	}
	public Integer getIsDefault() {
		return isDefault;
	}
	public void setIsDefault(Integer isDefault) {
		this.isDefault = isDefault;
	}
	
	public String getUserAccount() {
		return userAccount;
	}
	public void setUserAccount(String userAccount) {
		this.userAccount = userAccount;
	}
	
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	
	
}
