/*-----------------------------------------共用样式开始------------------------------------------------*/
#main{ margin:0 auto;width:1024px;height:100%}
/*头部*/
#head{height:90px; background:url(http://localhost:8080/static/images/headbg.png) no-repeat;}
a{font-family: "宋体","黑体","微软雅黑";font-size:12px;color: #56534A;}
div.logo{float:left;width:104px;height:90px; margin-left:50px; margin-top:10px }
div.top{float:right;width:724px;height:90px; }

div.menu{height:53px;width:580px;margin-left:120px; margin-top:20px; padding-top:10px;}
div.menu ul li{float:left; display:inline-block; width:80px;height:60px; padding:5px 0px 0 15px }
div.menu ul li a{font-size:14px; font-weight:bold}
div.menu ul li a:hover{ color:#005ca3; }
div.menu ul li.dq{ background-color:White;}
div.menu ul li.dq a{ color:#005ca3; }
/*已登录*/
div.sign{padding:10px 50px 0 400px;}
div.sign ul li{float:left; text-align:center}

/*中间*/
#mid{width:1024px; min-height:750px; padding-bottom:20px; border-bottom:1px solid #e2e3e4;}
/*底部*/
#bottom{width:958px;margin-left:30px;}
div.copyright{height:30px;font-size:13px; line-height:25px;padding:10px; text-align:center}
/*---------------------------------------------共用样式结束----------------------------------------------------*/
div.leftmenu{width:200px; border-right:3px solid #f5f5f6; min-height:400px;float:left }
div.headImg{padding:10px 20px 10px 30px}
div.nav{padding-left:30px}
div.nav ul{width:150px}
div.nav ul li{ line-height:28px; width:143px; padding-left:30px; padding-top:5px;  vertical-align:middle; float:left;  }
/*左边菜单背景图*/
div.nav ul li.s1{ background:url(http://localhost:8080/static/images/user/s1.png) no-repeat left;}
div.nav ul li.s2{ background:url(http://localhost:8080/static/images/user/s2.png) no-repeat left;}
div.nav ul li.s3{ background:url(http://localhost:8080/static/images/user/s3.png) no-repeat left;}
div.nav ul li.s4{ background:url(http://localhost:8080/static/images/user/s4.png) no-repeat left;}

 div.nav ul li.s5{ background:url(http://localhost:8080/static/images/security/point.png) no-repeat left;}
div.nav ul li.s6{ background:url(http://localhost:8080/static/images/security/posi.png) no-repeat left;}
div.nav ul li.s7{ background:url(http://localhost:8080/static/images/security/alarm.png) no-repeat left;} 

div.nav ul li.dq{ background-color:#e5e5e5;}
div.nav ul li a{font-size:15px; font-weight:bold }
/*右边内容--菜单*/
div.right{width:820px;float:right}
div.item{margin-left:20px}
div.item ul li{float:left; line-height:18px; padding-top:5px;  text-align:center; margin-top:5px}
div.item ul li.dq{border-bottom:3px solid #4397ce}
div.item ul li a{font-size:15px;color: #56534A;}
div.item ul li a:hover{ color:Red}
div.item ul li.shu{width:5px; padding:5px 20px 0 20px;border-bottom:none }

div.item2{margin-left:220px; }
div.item2 ul li{float:left;width:80px; line-height:18px; padding-top:5px;  text-align:center; margin-top:5px}
div.item2 ul li.dq{border-bottom:3px solid #4397ce}
div.item2 ul li a{font-size:15px;color: #56534A;}
div.item2 ul li a:hover{ color:Red}
div.item2 ul li.shu{width:5px; padding:5px 20px 0 20px;border-bottom:none }

div.item ul li.save{padding-left:30px;border-bottom:none;margin-top:0px}
/*右边内容--个人信息*/
div.info{margin:10px 10px 10px 20px;}
div.info span.label{font-size:13px; }
div.info span.input{padding-left:8px;text-align:left;  }
div.info select{width:107px}
div.info input{border: 1px solid #BBBBBB;font-family: arial,"宋体";font-size: 14px;height: 12px;padding: 4px 3px;width: 240px;margin-left:2px;}
div.info input.t1{width:100px;}
div.info input.t2{width:20px; height:15px;border:none }
div.info input.t3{width:230px}
div.info input.t4{width:330px}
div.info div{ line-height:30px;}
span.LB1{padding-right:38px; }
span.LB2{padding-right:10px; }
span.LB3{padding-right:13px; }
span.LB4{padding-right:23px; }
/*安全监测---查看位置*/
div.info ul{margin-left:50px;}
div.info ul li{float:left;width:310px; line-height:35px; padding-top:5px;  text-align:left; margin-top:5px; padding-left:60px;}
div.info ul li span{display:block}
div.info ul li span.title{font-size: 14px;color:#2b72a8;font-weight:bold}
div.info ul li span.content{font-size: 14px;color:#585858;font-weight:bold}
div.info ul li.p1{ background:url(http://localhost:8080/static/images/security/p1.png) no-repeat left;}
div.info ul li.p2{ background:url(http://localhost:8080/static/images/security/p2.png) no-repeat left;}
div.info ul li.p3{ background:url(http://localhost:8080/static/images/security/p3.png) no-repeat left;}
div.info ul li.p4{ background:url(http://localhost:8080/static/images/security/p4.png) no-repeat left;}
div.info ul li.p5{ background:url(http://localhost:8080/static/images/security/p5.png) no-repeat left;}
div.info ul li.p6{ background:url(http://localhost:8080/static/images/security/p6.png) no-repeat left;}
div.info ul li.p7{ background:url(http://localhost:8080/static/images/security/p7.png) no-repeat left;}
div.info ul li.p8{ background:url(http://localhost:8080/static/images/security/p8.png) no-repeat left;}
div.info ul li.p9{ background:url(http://localhost:8080/static/images/security/p9.png) no-repeat left;}
div.info ul li.p10{ background:url(http://localhost:8080/static/images/security/p10.png) no-repeat left;}
div.info ul li.p11{ background:url(http://localhost:8080/static/images/security/p11.png) no-repeat left;}
div.info table{height:100%;width:770px}

div.info table tr td,div.info table tr th{border:1px solid #e4e4e4;font-weight:bold; text-align:center}
div.info table tr th{color:#2b72a8;font-size: 14px;line-height:25px; }
div.info table tr td{color:#585858; font-size: 12px;height:20px;padding:2px 0;}

div.info table.relative-tel{width:600px}
div.info table.relative-tel tr td,div.info table tr th{border:0px solid #e4e4e4;font-weight:bold; text-align:center}
div.info table.relative-tel tr th{color:#585858;font-size: 12px;line-height:25px; }
div.info table.relative-tel tr td{color:#585858; font-size: 12px;height:20px;padding:2px 0;}
div.info table.relative-tel tr td input{width:150px;border:none}
div.info table.relative-tel tr td input.text{ border:1px solid #dad8d8;}

