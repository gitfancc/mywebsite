.chart img{
	border:1px solid #ccc;
	*margin-top:-1px; 
	-moz-box-shadow:5px 5px 4px #eee inset ;              
	-webkit-box-shadow:5px 5px 4px #eee inset;           
			box-shadow:3px 2px 4px #eee ;
