}
.mapbox .foot{
	background: #999;
	width: 700px;
	color: #000;
	position: absolute;
	top: 400px;
	left: 0;
	padding: 10px 0;
	-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=70)"; /* ie8  */
	filter: alpha(opacity=70);    /* ie5-7  */
	opacity: 0.7;    /* css standard, currently it works in most modern browsers  */
}
