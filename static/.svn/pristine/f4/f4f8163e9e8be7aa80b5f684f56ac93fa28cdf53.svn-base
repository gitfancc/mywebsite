.slideTxtBox .hd span{ position:absolute; left:30px; top:10px;}
.slideTxtBox .hd ul{ position:absolute; left:70px; top:-1px;}
.slideTxtBox .hd ul li{ 
	float:left; 
	text-align:center; 
	width:120px;
	height:35px; 
	line-height:35px; 
	cursor:pointer; 
	background:url(../images/btn-bg.gif) no-repeat;
}
.slideTxtBox .hd ul li a:hover{ color:#666;}
.slideTxtBox .hd ul li.on{ background:url(../images/btn-bg.gif) 0 -35px no-repeat; color:#FFF;}
.slideTxtBox .hd ul li.on a{ color:#FFF;}
.slideTxtBox .bd{ margin-top:10px;}
.slideTxtBox .bd ul{ padding:15px 0;  zoom:1;  }
.slideTxtBox .bd li{ height:24px; line-height:24px;  }
.slideTxtBox .bd li .date{ float:right; color:#999;}
.mt-n{_margin-top:-15px;}
.tip-confirm{ 
	background:url(../images/tip_confirm.gif) 0 center no-repeat; 
	padding-left:65px;
	line-height:65px;
	height:65px;
	font-size:20px;
}
.tip-confirm-pos{margin-left:150px;}

/*404页面*/
.error{
	background:#f2f5f6;
	height:420px;
	position:relative;
}
.error .img-icon{position:absolute; top:90px;left:120px;}
.error .img-txt{position:absolute; top:70px;left:445px;}
.error .img-divide{position:absolute; top:70px;left:330px;}
.error .link{
	position:absolute;
	top:255px;
	left:450px;
	font-size:14px;
	width:200px;
}
.error .link p{line-height:28px;}
/*legal页面*/
.legal{ padding:20px 30px 30px 30px;}
.legal p{
	text-align:justify;
	line-height:18px;
	margin:20px 0;
}
.legal h2{ 
	font-size:20px; 
	color:#005ca3;
	font-weight:bold;
	font-family:"微软雅黑", "宋体";
	text-align:center;
	margin-bottom:20px;
}
.legal h3,
.legal h4{
	font-size:16px;
	font-weight:bold;
	margin-bottom:20px;
}
