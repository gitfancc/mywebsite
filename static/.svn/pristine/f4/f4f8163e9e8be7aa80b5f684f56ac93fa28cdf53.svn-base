/*首页的slider样式 */
.slideBox{
	width:895px;
	height:315px;
	overflow:hidden;
	position:relative;
	background:#fff;
	margin:0 auto;
}
.slideBox .hd{
	overflow:hidden;
	position:absolute;
	left:410px;
	bottom:5px;
	z-index:1;
}
.slideBox .hd ul{ overflow:hidden; zoom:1; float:left;  }
.slideBox .hd ul li{ 
	float:left; 
	margin-right:4px;
	height:14px;
	width:14px;  
	cursor:pointer;
	background:url(../images/btn_slider_page.gif) 0 -14px no-repeat;
}
.slideBox .hd ul li.on{background:url(../images/btn_slider_page.gif)  no-repeat;}
.slideBox .bd{ position:relative; height:100%; z-index:0; width:950px; margin:0 auto;}
.slideBox .bd li{ zoom:1; vertical-align:middle; }
.slideBox .bd img{ width:895px; height:315px; display:block;  }

