.ui-step {
    padding: 0 40px;
    font-size: 14px;
    list-style: none;
    zoom: 1;
    position: relative;
    color: #b7b7b7;
    overflow: hidden;
	margin:30px auto;
}

.ui-step:after {
    clear: both;
    content: " ";
    display: block;
    font-size: 0;
    height: 0;
    visibility: hidden;
}

.ui-step li {
    float: left;
    height: 60px;
    margin: 0;
    width: 50%;
    position: relative;
}

.ui-step li .ui-step-icon {
    height: 50px;
    position: absolute;
    top: 0;
    text-align: center;
}
.ui-step .ui-step-icon .icon{
	width:34px;
	height:34px;	
}
.ui-step .ui-step-icon .icon-step-done{
	background:url(../images/step.jpg) no-repeat;
}
.ui-step .ui-step-icon .icon-step-active{
	background:url(../images/step.jpg) -34px 0 no-repeat;
}
.ui-step .ui-step-icon .icon-step-end{
	background:url(../images/step.jpg) -68px 0 no-repeat;
}
.ui-step .ui-step-icon .icon-step-end-active{
	background:url(../images/step.jpg) -102px 0 no-repeat;
}

.ui-step-icon .ui-step-number {
    line-height:14px;
    font-style:normal;
    position: absolute;
    top: 10px;
    left: -6px;
    padding: 0 20px;
    font-weight: bold;
    color: #fff;
}

.ui-step .ui-step-active i.ui-step-number {
    color:#FFF;
}

.ui-step-end .ui-step-number {
    background: transparent;
    font-size: 12px;
    font-style: normal;
    left: 17px;
    line-height: 12px;
    padding: 0;
    text-shadow: none;
    top: 9px;
}

.ui-step-icon .ui-step-text {
    position: relative;
    text-shadow: 0 1px 0 white;
    top: 38px;
    left: -55px;
}

.ui-step li .ui-step-line {
    height: 3px;
    background: #b9b9b9;
    display: block;
    margin-top: 17px;
    line-height: 1;
    overflow: hidden;
}

.ui-step li.ui-step-end {
    position: absolute;
    top: 0;
    right: -110px;
    width: 150px;
    zoom: 1;
    _clear: both;   /* 修复ie6下最后一步莫名其妙显示不出来的问题 */
}

.ui-step li.ui-step-end .ui-step-line {
    display: none;
}

.ui-step .ui-step-active {
    color: #3b7cb8;
}

.ui-step .ui-step-done .ui-step-line {
    background: #2bc0c2;
}
