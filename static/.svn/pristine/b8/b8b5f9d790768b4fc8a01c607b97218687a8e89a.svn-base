	<div class="hero-unit skybox">
        <div class="regbox">
        <form>
            <fieldset>
            <legend>免费注册成为乐康会员</legend>
                <div>
                     <label for="user">账号：</label>
                     <input type="text" id="user"  placeholder="手机号/邮箱">
                     <span class="text-error"><i class="icon-remove"></i> 请输入正确的邮箱号码</span>
                </div>
               <div class="control-group">
               		<label for="nickname">呢称：</label>
                    <input type="text"  id="nickname" placeholder="请输入呢称">
                    <span class="text-error"><i class="icon-remove"></i> 呢称不能为空</span>
                </div>
                <div class="control-group">
               		<label for="password">密码：</label>
                    <input type="text"  id="password" placeholder="请输入密码">
                    <span class=" text-success"><i class="icon-ok"></i> 密码区分大小写</span>
                </div>
                <div class="control-group">
               		<label for="confirmpassword">确认密码：</label>
                    <input type="text"  id="confirmpassword" placeholder="请再次输入密码">
                    <span class=" text-success"><i class="icon-ok"></i> 请再次输入密码</span>
                </div>
              	<div class="control-group">
                    <label class="checkbox"><input type="checkbox">我已经阅读并接受 &nbsp;<a href="服务条款.html">乐康网服务条款</a></label>
                 </div>   
                <button type="submit" class="btn btn-large btn-success span4">确认注册</button>
            </fieldset>
        </form>
